      $(".lsc-loan-amount").text('MYR' + commaSeparateNumber(lsclamount));
      $(".lsc-interest-rate").text(lscirate + '%');
      var C = $('form[name="loansettle"] [name="lscloanamount"]').val() * $('form[name="loansettle"] [name="lscintrate"]').val() / 100 * $('form[name="loansettle"] [name="lscfullperiod"]').val() / 12;
      if (!C) {
        $(".lsc-result").css("display", "none");
        $(".lscapplybtn").css("display", "none");
      } else {
        $(".lsc-result").css("display", "block");
        $(".lscapplybtn").css("display", "block");
      }

      var n = T - P - 1;
      var T1 = n * (n + 1);
      var T2 = eval(T) * (eval(T) + 1);
      var T3 = T1 / T2 * C;
      $(".lsc-total-rebate-amount").text('MYR' + commaSeparateNumber(lsccalcRound(T3)));
      var G1 = lsccalcRound(eval(C) + eval(lsclamount) - eval(P) * eval(lscinstallment) - eval(T3));
      $(".lsc-total-pay-finance-amount").text('MYR' + commaSeparateNumber(lsccalcRound(G1)));
      window.scrollBy(0, 200);
      $(".loansettlementcalculator .hidden-result span").text(lsclamount + ' ' + '|' + ' ' + lscirate + '%' + ' ' + '|' + ' ' + T + ' ' + '|' + ' ' + P + ' ' + '|' + ' ' + lscinstallment);
      if (isNaN($('form[name="loansettle"] [name="lsctotalrebate"]').val())) {
        $('form[name="loansettle"] [name="lsctotalrebate"]').val(0);
      }
      if (isNaN($('form[name="loansettle"] [name="lscsettleamount"]').val())) {
        $('form[name="loansettle"] [name="lscsettleamount"]').val(0);
      }
    }
  };

  function commaSeparateNumber(val) {
    while (/(\d+)(\d{3})/.test(val.toString())) {
      val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
    }
    return val;
  }
  function lsccalcRound(num1) {
    var lscresult = Math.floor(num1) + ".";
    var lscn = lscresult.length;
    var lsccents = 100 * (num1 - Math.floor(num1)) + 0.5;
    lscresult += Math.floor(lsccents / 10);
    lscresult += Math.floor(lsccents % 10);
    return lscresult;
  }

  function alertMsg(field, msg) {
    alert(msg);
    field.focus();
    field.select();
  }

  function validDigit(ch) {
    if (ch != "." && ch != "0" && ch != "1" && ch != "2" && ch != "3" && ch != "4" && ch != "5" && ch != "6" && ch != "7" && ch != "8" && ch != "9") {
      return false;
    } else {
      return true;
    }
  }

  function digit(string) {
    var i;
    var chr;
    for (i = 0; i < string.length; i++) {
      chr = string.substring(i, i + 1);
      if (!validDigit(chr)) {
        return false;
      }
    }
    return true;
  }

  function empty(string) {
    if (string.length == 0) {
      return true;
    } else {
      for (i = 0; i < string.length; i++) {
        ch = string.substring(i, i + 1);
        if (ch != " ") {
          return false;
        }
      }
      return true;
    }
  }

  function check_empty(field, alert_msg) {
    if (empty(field.value)) {
      alertMsg(field, alert_msg);
      return false;
    }
    return true;
  }
})();
