(function () {

  "use strict";

  $(document).ready(function () {
    var counter = 0,
      max = 3,
      income = 0;

    // Select interest
    $('.recommender-interest-block').on('click', function (e) {
      e.preventDefault();
      if (counter < max && !$(this).hasClass('selected')) {
        counter++;
        $(this).addClass('selected');
      } else if ($(this).hasClass('selected')) {
        counter--;
        $(this).removeClass('selected');
      } else {
        return false;
      }

      //Revamp Logic
      if (counter >= max) {
        $(".benefits-categories-wrapper").addClass("max-selection");
      } else {
        $(".benefits-categories-wrapper").removeClass("max-selection");
      }

      checkSelected();
    });

    // Money format 1,234.56
