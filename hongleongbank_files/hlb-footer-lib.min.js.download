(function () {
  "use strict";

  window.translatei18n = function (className) {
      var values = [];
      var translatedValues = [];

      $('.' + className).each(function() {
          values.push($(this).text());
      });

      $.ajax({
          async: false,
          type: 'GET',
          url: '/bin/hlb/TranslationUtil',
          cache: false,
          data: {
              values: values,
              locale: locale
          },
          success: function(obj) {
              translatedValues = obj;
          },
          error: function(obj) {
              console.log("[/bin/hlb/TranslationUtil] AJAX Fail");
          }
      });

      var count = 0;
      $('.' + className).each(function() {
          $(this).text(translatedValues[count]);
          count++;
      });
  }
})();
