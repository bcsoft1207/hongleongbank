    // Recommender form
    var form = $('#credit-card-recommender');

    $('#monthly_income').on('focus', function (e) {
      if ($(this).val() == 0) {
        $(this).val('');
      }
    }).on('change', function () {
      var amount = incomeAmount(income);
      if (amount > 1) {
        $('.recommender-interest-wrapper').removeClass('cover');
      } else {
        if ($(".credit-card-recommender-revamp").length == 0) {
          clearResultAndSetting();
        }

        $('#monthly_income').val(0);
        $('.recommender-interest-wrapper').addClass('cover');
      }
