function loadTextImageVideoYT() {
  $(".text-image-video").each(function (index) {
    var elem = $(this);
    var textVideoYtId;
    var textVideoPlayer = undefined;

    if (elem.find(".video-url").val()) {
      if (elem.find(".is-video-url").val() == "n") {
        textVideoYtId = elem.find(".video-url").val();
      } else {
        var ytRx = /^.*(?:youtu.be\/|v\/|e\/|u\/\w+\/|embed\/|v=)([^#\&\?]*).*/;
        textVideoYtId = elem.find(".video-url").val().match(ytRx)[1];
      }

      elem.find('.image-video').attr({
        'data-src': 'https://img.youtube.com/vi/' + textVideoYtId + '/sddefault.jpg'
      });


      $(".image-video.lazy").lazy();

      elem.find('.video-trigger').on('click', function () {
        elem.find('.video-trigger').hide();
        textVideoPlayer = new YT.Player(elem.find('.videoPlayer')[0], {
          videoId: textVideoYtId,
          playerVars: {
            "showinfo": 0,
            "rel": 0
          },
          events: {
            'onReady': function () {
              textVideoPlayer.playVideo();
            }
          }
        });
      });
    }
  });
}

