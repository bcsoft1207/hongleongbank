$(function () {
  "use strict";

  var parent = undefined;
  $('.panel-group').on('shown.bs.collapse', function () {
    var $instance = $(this);

    if (parent) {
      parent = ($(this).parent().closest('.panel-group').length > 0) ? $(this).parent().closest('.panel-group') : undefined;
      return false;
    }

    parent = ($(this).parent().closest('.panel-group').length > 0) ? $(this).parent().closest('.panel-group') : undefined;

    var expanded = undefined;
    $($(this).find('[aria-expanded=true]')).each(function (ind) {
      if ($(this).closest('.panel-group')[0] == $instance[0]) {
        expanded = $(this)
        return false;
      }
    });

    var activePosition = expanded.closest('.panel').offset().top - $('.header-bar-overall').height();

    var isMobile = window.matchMedia("only screen and (max-width: 760px)");
    if (isMobile.matches && $('.sticky.cta-btn').length > 0) {
      var stickyBtn = $('.sticky.cta-btn');
      var stickyHeight = stickyBtn.attr('data-sticky-height') | 0;
      activePosition = activePosition - stickyHeight;

      var offset = stickyBtn.offset().top;
      if (activePosition >= offset) {
        if (!stickyBtn.hasClass('fixed')) {
          activePosition = (activePosition - (stickyHeight * 2) > offset) ? activePosition - (stickyHeight * 2) : offset;
        }
      }
    }
    //$(window).scrollTop(activePosition);
    var completeCalled = false;
    $("html, body").animate({
      scrollTop: activePosition
    }, {
      complete: function () {
        if (!completeCalled) {
          completeCalled = true;
        }
      }
    });
  })
})

