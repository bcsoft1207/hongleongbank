      var types = ['Premium', 'Cash Back', 'Business', 'Rewards', 'No Frill'];
      var contIds = ['premium', 'cash-back', 'business', 'rewards', 'no-frill'];

      return contIds[types.indexOf(cardType)];
    }

    function updateAddCardButtonState(card, state) {
      var typeName = card.type.toLowerCase().replace(' ', '-');
      var contId = '#compare-card-' + typeName;
      switch (state) {
