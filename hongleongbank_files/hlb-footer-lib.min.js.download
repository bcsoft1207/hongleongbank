      // Selection on filter lvl 2
      $(document).on('click', '.search-categories-filter-lvl-2 .search-categories-filter-block', function (e) {
        e.preventDefault();
        $('.search-categories-filter-lvl-2 .search-categories-filter-block').removeClass('selected');
        $(this).addClass('selected');
      });

      // Show and hide show more button
      $(document).on('click', '.search-container .cta-show-more', function (e) {
        e.preventDefault();
