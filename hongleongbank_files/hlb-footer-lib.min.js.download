function submitAnswer(hexPath) {
  var form = $("form[data-form-h='" + hexPath + "']");
  var allow = true;
  var formData = {};
  formData['name'] = form.attr('data-form-name') ? form.attr('data-form-name') : '';
  formData['hex'] = form.attr('data-form-h') ? form.attr('data-form-h') : '';
  formData['sha'] = form.attr('data-form-s') ? form.attr('data-form-s') : '';
  formData['fields'] = [];
  $('.quiz-answer-error, .survey-answer-error').addClass('hidden');
  form.find('div[data-input-t]').each(function () {
    if (!$(this).attr('data-input-t')) {
      allow = false;
      $(this).siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#errMandatory').val());
      $(this).siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').removeClass('hidden');
    } else {
      var fieldVal = {};
      fieldVal['hex'] = $(this).attr('data-input-h');
      fieldVal['token'] = $(this).attr('data-input-t');
      if (typeof $(this).closest('.quiz-question, .survey-question').find('h6').data('headline') !== 'undefined') {
        fieldVal['question'] = $(this).closest('.quiz-question, .survey-question').find('h6').data('headline');
      }else{
        fieldVal['question'] = $(this).closest('.quiz-question, .survey-question').find('h6').text();
      }      
      fieldVal['answers'] = [];

      if ($(this).find('input[data-input-s]').length > 1 || $(this).find('input[data-input-s][type=checkbox]').length > 0 || $(this).find('input[data-input-s][type=radio]').length > 0 || $(this).find('option[data-input-s]').length > 0) {
        // if ($(this).find('input[data-input-s]').length > 1 | $(this).find('input[type=checkbox]').length > 0 || $(this).find('input[type=radio]').length > 0) {
        if ($(this).find('input[data-input-s]').attr('data-input-mandatory') == '0' ||
          ($(this).find('input[data-input-s]').attr('data-input-mandatory') == '1' && $(this).find('input[data-input-s]:checked').length > 0)) {

          $(this).find('input[data-input-s]:checked').each(function () {
            var ans = {};
            ans['sha'] = $(this).attr('data-input-s');
            ans['answer'] = $(this).val();

            fieldVal['answers'].push(ans);
          });
          // radio with optional text box
          var opt_input = $(this).find('input[data-input-s]:checked').parents(".opt-wrapper").find('input[type=text]');
          if($(opt_input).data('optional') == true){
            if($(opt_input).attr('data-input-mandatory') == '0' ||
            ($(opt_input).attr('data-input-mandatory') == '1' && $(opt_input).val())){
              var ans = {};
              ans['sha'] = $(opt_input).attr('data-input-s');
              ans['answer'] = $(opt_input).val();
              fieldVal['answers'].push(ans);
              //$(opt_input).closest(".optInput").siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').addClass('hidden');
            }else {
              allow = false;
              $(opt_input).closest(".optInput").siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#errMandatory').val());
              $(opt_input).closest(".optInput").siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').removeClass('hidden');
            }
          }
        }else if($(this).find('option[data-input-s]').attr('data-input-mandatory') == '0' ||
          ($(this).find('option[data-input-s]').attr('data-input-mandatory') == '1' && $(this).find('option[data-input-s]:selected').length > 0)){

            var ans = {};
            ans['sha'] = $(this).find('option[data-input-s]:selected').attr('data-input-s');
            ans['answer'] = $(this).find('option[data-input-s]:selected').val();

            fieldVal['answers'].push(ans);
        } else {
          allow = false;
          $(this).siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#errMandatory').val());
          $(this).siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').removeClass('hidden');
        }
      } else {
        if (($(this).find('input[data-input-s]').attr('data-input-mandatory') == '0' ||
          ($(this).find('input[data-input-s]').attr('data-input-mandatory') == '1' && $(this).find('input[data-input-s]').val())) && $(this).find('input[data-input-s]').data('optional') !== true) {

          var ans = {};
          ans['sha'] = $(this).find('input[data-input-s]').attr('data-input-s');
          ans['answer'] = $(this).find('input[data-input-s]').val();

          fieldVal['answers'].push(ans);
        } else {
          allow = false;
          $(this).siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#errMandatory').val());
          $(this).siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').removeClass('hidden');
        }
      }

      formData['fields'].push(fieldVal);
    }
  });

  if (allow) {

    var sendtocustomer = "n";
    if ($("#sendEmailToClient").is(":checked")) {
      sendtocustomer = "y";
    }


    $.ajax({
      type: 'POST',
      url: '/bin/hlb/surveyquiz.json',
      cache: false,
      data: {
        'data': JSON.stringify(formData),
        'sendemail': sendtocustomer
      },
      success: function success(obj) {
        if (obj.errorCode == "000") {
          if (obj.result.formType == "01") //quiz
          {
            $('#survey-quiz-container').show();
            $('#quiz').show();
            $('#survey').hide();
            $('#form').hide();
            $('#submit').hide();
            $('#score').text(obj.result.scoreLabel);
            $('#resultLabel').text(obj.result.resultLabel);
            $('#resultLabel').css('color', obj.result.rankingColor);
            $('#rankingLabel').text(obj.result.rankingLabel);

          } else if (obj.result.formType == "02") //survey
          {
            $('#survey-quiz-container').show();
            $('#survey').show();
            $('#quiz').hide();
            $('#form').hide();
            $('#submit').hide();
          }
          window.scrollTo(0, 0);
        } else if (obj.errorCode == "102") {
          var emailInvalid = false;
          var mandatoryError = false;
          var numericError = false;

          for (key in obj.invalid) {
            var errorElem = $('div[data-input-h="' + key + '"]');
            if (obj.invalid[key] == "5") {
              errorElem.siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#'+key+'-errMatch').val());
            } else if (obj.invalid[key] == "4") {
              errorElem.siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#'+key+'-errRegex').val());
            } else if (obj.invalid[key] == "3") {
              numericError = true;
              errorElem.siblings('.quiz-answer, .survey-answer').find('.quiz-answer-error, .survey-answer-error').text($('#errNumeric').val());
            } else if (obj.invalid[key] == "2") {
