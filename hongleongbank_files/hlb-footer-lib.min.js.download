(function () {

	"use strict";

	window.store = {};
	window.Helpers = {
		getDataListFromSession: function getDataListFromSession(key) {
			var data = JSON.parse(localStorage.getItem(key) || '[]');
			if (Array.isArray(data)) {
				return data;
			}
			return null;
		},
		addDataListToSession: function addDataListToSession(key, item) {
			if (Array.isArray(item)) localStorage.setItem(key, JSON.stringify(item));
		},

		triggerChangedEvent: function triggerChangedEvent(listName, data) {
			var evt = new CustomEvent(listName, { detail: JSON.parse(localStorage.getItem(listName) || '[]') });
			dispatchEvent(evt);
		}
	};

	/* STORE AN ITEM */
	store = Object.create(Helpers);
	store.addListItem = function (listName, item, silent) {
		var items = this.getDataListFromSession(listName);
		if (Array.isArray(items)) {
			items.push(item);
			this.addDataListToSession(listName, items);
		} else {
			this.addDataListToSession(listName, [item]);
		}

		if (!silent) this.triggerChangedEvent(listName, { addedItem: item });
	};
	store.getAllFromList = function (listName) {
		var items = this.getDataListFromSession(listName);
		return items;
	};
	store.clearList = function (listName) {
		this.addDataListToSession(listName, []);
	}, store.getListItem = function (listName, id) {
		var items = this.getDataListFromSession(listName);
		if (Array.isArray(items)) {
			for (var i = 0; i < items.length; i++) {
				if (items[i].id == id) {
					return items[i];
				}
			}
		}
		return null;
	};
	store.removeListItem = function (listName, id) {
		var items = this.getDataListFromSession(listName);
		if (Array.isArray(items)) {
			var updatedlist = [];
			var toRemove = [];
			items.forEach(function (i) {
				if (i.id != id) {
					updatedlist.push(i);
				} else {
					toRemove.push(i);
				}
			});

			this.addDataListToSession(listName, updatedlist);
			if (items.length != updatedlist.length) this.triggerChangedEvent(listName, { removedItem: toRemove[0] ? toRemove[0] : null });
		}
	};
	store.addMultipleListItems = function (listName, items) {
		var _this = this;
		if (Array.isArray(items)) {
			items.forEach(function (item) {
				_this.addListItem(listName, item, true);
			});
		}

		this.triggerChangedEvent(listName);
	};
	store.addToCompare = function (id) {
		if (this.getListItem('comparison', id)) {
			alert('This card already in comparison list. Please try a differen one.');
			return false;
		}
		var compareList = this.getAllFromList('comparison');
		var card = this.getListItem('cards', id);
		if (compareList.length >= 3) {
			//alert('Already selected three cards to compare. Please remove and try again.');
            console.log('hereee');
			return false;
		}
		this.addListItem('comparison', card);

		return true;
	};
	store.removeFromCompare = function (id) {
		if (!this.getListItem('comparison', id)) {
			alert('The card is not in the comparison list.');
			return false;
		}
		this.removeListItem('comparison', id);

		var compareList = this.getAllFromList('comparison');
	};
})();



