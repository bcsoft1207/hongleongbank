      var key = e.charCode || e.keyCode;
      if (key == 13) {
        e.preventDefault();
        var amount = incomeAmount(income);
        if (amount > 1) {
          $('.recommender-interest-wrapper').removeClass('cover');
        } else {
          if ($(".credit-card-recommender-revamp").length == 0) {
            clearResultAndSetting();
          }
          $('#monthly_income').val(0);
