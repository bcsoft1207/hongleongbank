  navSwitcher();

  function startScrolling(obj, param, horizontal) {
    var options = {};
    if (horizontal) {
      options.scrollLeft = param;
    } else {
      options.scrollTop = param;
    }
    obj.animate(options, "fast", 'linear', function () {
      if (scrolling) {
        startScrolling(obj, param, horizontal);
      }
    });
  }

  /* YEAR SCROLLER EVENT HANDLERS */
  $(document).on('mousedown', '#year-list-scroll-up-btn', function (e) {
    scrolling = true;
    var horizontal = $('.scrollable-year-list-container').hasClass('horizontal');
    startScrolling($('.scrollable-year-list'), '-=50', horizontal);
    return false;
  }).on('mouseup mouseleave', '#year-list-scroll-up-btn', function (e) {
    scrolling = false;
    return false;
  });

  $(document).on('mousedown', '#year-list-scroll-down-btn', function (e) {
    scrolling = true;
    var horizontal = $('.scrollable-year-list-container').hasClass('horizontal');
    startScrolling($('.scrollable-year-list'), '+=50', horizontal);
    return false;
  }).on('mouseup mouseleave', '#year-list-scroll-down-btn', function (e) {
    scrolling = false;
    return false;
  });

  /* END YEAR SCROLLER EVENT HANDLERS */

  $(window).on('resize', function () {
    navSwitcher();
  });

  $(document).on('click', '.menu-dropdown-tab-switcher .tab-nav-bar > li > a', function (e) {
    var current = $(this);
    if (!current.is('li')) {
      current = current.parent('li');
    }
    current.closest('.tab-nav-bar').find('li').removeClass('active');
    current.addClass('active');
    current.closest('.menu-dropdown-tab-switcher').find('.dropdown-btn-label').html(current.html());
  });

  function renderPageContent(data) {
    var raw_template = $('#template-bursa-news-page-content').html();
    if (!raw_template) return; // not in comparison page.
    var target = $('#bursa-page-content');
    var template = Handlebars.compile(raw_template);
    target.html(template(data));
  }

  function getActiveYear() {
    var active = $('.scrollable-year-list > li.active');
    if (active.length == 0) {
      active = $('.scrollable-year-list > li').first().addClass('active');
    }

    return active.find('a').prop('hash').replace('#', '');
  }

  function fetchDataByYear(year) {
    $.ajax({
      method: 'GET',
      //url: 'data/years/'+ year +'/pdf-download-list.json', //this link is only for development. need to replace it in production
      url: '/bin/hlb/pdfReports?componentPath=' + componentPath + '&year=' + year,
      dataType: 'json'
    }).done(function (result) {
      if (!result) return;
      renderPageContent(result);
    });
  }

  function navSwitcher() {
    if ($(window).width() < 768) {
      $('.menu-dropdown-tab-switcher').find('.navbar-nav').addClass('dropdown-menu').removeClass('navbar-nav nav');
      $('.scrollable-year-list-container').addClass('horizontal');
    } else {
      $('.menu-dropdown-tab-switcher').find('.dropdown-menu').addClass('nav navbar-nav').removeClass('dropdown-menu');
      $('.scrollable-year-list-container').removeClass('horizontal');
    }
  }

  $(document).on('click', '.scrollable-year-list > li > a', function () {
    var current = $(this);
    $('.scrollable-year-list > li').removeClass('active');
    current.parent('li').addClass('active');

    fetchDataByYear(current.prop('hash').replace('#', ''));
  });

  // initial data render bursa
  /*
      setting data
  */
  //============================================================
  if ($('.scrollable-year-list-container').length > 0) {
    fetchDataByYear(getActiveYear());
  }

  // ============================================================


  /*BACK TO TOP BUTTON*/
  $(document).on('click', '.back-to-top-button', function () {
    $('html, body').animate({
      scrollTop: 0
    }, 'slow');
  });
});
