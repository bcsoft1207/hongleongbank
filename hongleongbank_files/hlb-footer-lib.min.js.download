      if (counter == 0) {
        $('#credit-card-recommender button[type="submit"]').prop('disabled', true);
        $("#ctaButton").prop('disabled', true);
      } else {
        $('#credit-card-recommender button[type="submit"]').prop('disabled', false);
        try {
          var number = parseFloat($('.recommender-monthly-income-input').val().replace(/,/g, ''));
          number = Math.round(number);

          if (number > 0) {
            $("#ctaButton").prop('disabled', false);
          } else {
            number = Math.round(number);
          }
        } catch (e) { }
      }
    }

    function clearResultAndSetting() {
      $('.recommender-interest-block').removeClass('selected');
      $('.recommender-result-header').html('');
      $('.recommender-result-wrapper').html('');
      counter = 0;
      checkSelected();
    }

    function renderRecommenderResult() {
      $('.recommender-result-wrapper').html('');
      $('.recommender-result-wrapper').append(html);
    }

    function createArray(itemArr) {
      var h,
        selectedItem = $('.recommender-interest-block.selected'),
        lenInterest,
        item = "";
      for (h = 0, lenInterest = selectedItem.length; h < lenInterest; h++) {
        item = String($(selectedItem[h]).attr('data-interest'));
        itemArr.push(item);
      }
      return itemArr;
    }

    function incomeAmount(income) {
      var number = parseFloat($('.recommender-monthly-income-input').val().replace(/,/g, ''));
      return income = Math.round(number);
    }

    function printHeader(header) {
      var header = "";
      $('.recommender-result-header').html('');
      return header = $('.recommender-result-header').append($('.lblResultHeader').val());
    }
  });
})();

