// Get URL Parameters
function GetURLParameter(sParam) {
  var sPageURL = window.location.search.substring(1);
  var sURLVariables = sPageURL.split('&');
  for (var i = 0; i < sURLVariables.length; i++) {
    var sParameterName = sURLVariables[i].split('=');
    if (sParameterName[0] == sParam) {
      return sParameterName[1];
    }
  }
}


//Get unicode
function getRegexUnicode(sParam) {
  var uniCode = "";
  for (var i = 0; i < sParam.length; i++) {
    var charCode = sParam.charCodeAt(i);
    var hexStr = charCode.toString(16);
    if (hexStr.length < 4) {
      for (var j = hexStr.length; j < 4; j++) {
        hexStr = "0" + hexStr;
      }
    }
    uniCode += "\\u" + hexStr;
  }
  return uniCode;
}

function initCompareCard() {

  if (typeof (creditCardRootPath) == "undefined") {
    creditCardRootPath = "/content/hlb/my/en/personal-banking/credit-cards";
  }

  if (!Helpers.getDataListFromSession('cards') || Helpers.getDataListFromSession('cards').length <= 0) {
    $.ajax({
      method: 'GET',
      url: '/bin/hlb/compareCards?creditCardRootPath=' + creditCardRootPath + '&locale=' + locale,
      dataType: 'json'
    }).done(function (result) {
      if (!result) return;
      store.clearList('cards');
      store.addMultipleListItems('cards', result);
      Helpers.triggerChangedEvent('cards');
    });
  } else {
    Helpers.triggerChangedEvent('cards');
  }
}

function projectldirectUrl(ele) {
  var json, form, input;
  var url = $(ele).attr("data-url");
  form = document.createElement("form");
  form.method = "post";
  form.action = url.split("?")[0];
  if (window.location.href.indexOf('#applyNow') == -1) {
    form.target = "_blank";
  }
  var ecp = "";
  var urlParams = $.URLSearchParams(window.location.search);

  var url_vars = [],
    hash;
  var hashes = url.slice(url.indexOf('?') + 1).split('&');
  for (var i = 0; i < hashes.length; i++) {
    hash = hashes[i].split('=');
    url_vars[hash[0]] = hash[1];
  }

  for (var i in url_vars) {
    if (i === "ecp") {
      form.action += "?ecp=" + url_vars[i];
    } else {
      var input = document.createElement("input");
      input.setAttribute("name", i);
      input.setAttribute("value", url_vars[i]);
      form.appendChild(input);
    }
  }
  //      console.log(form.action);
  document.body.appendChild(form);
  form.submit();
  document.body.removeChild(form)

};

function checkSecureLink(url) {
  var urlProjectL = url;
  var isProjectLUrl = false;


  if ($("#global-footer").length > 0) {

    var accountname = $("#global-footer").data("amountparam");
    var projectLConfig = $("#global-footer").data("projectlconfig");

    if (projectLConfig.length > 0) {
      var accountname = $("#global-footer").data("amountparam") || "inc"; // added
      var monthlyIncome = localStorage.getItem(accountname) || 0;
      
	  for(var i=0; i<projectLConfig.length; i++){
        var config = projectLConfig[i];
        var excludeDomainName = config.excludeDomainName;
        excludeDomainName = excludeDomainName.replace(/ /g, '');

        if(urlProjectL !== "" && urlProjectL.indexOf(excludeDomainName) > -1){
          isProjectLUrl = true;
          
          if(config.includeAmountParameter == 'true' && monthlyIncome !== 0){

            if (urlProjectL.indexOf("?") >= 0) {
              urlProjectL = urlProjectL + "&" + accountname + "=" + monthlyIncome;
            } else {
              urlProjectL = urlProjectL + "?" + accountname + "=" + monthlyIncome;
            }

          }

        }
      }
    }
  }
  return [urlProjectL, isProjectLUrl];

}

