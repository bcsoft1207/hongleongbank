        $(this).closest('.filter-expand-wrapper').toggleClass('hidden');
        $('.credit-card-rewards-result-item-group').empty();
        call = true;
        dataSubmit();
        ajaxPull();
        backToTop();
      });
    }

    if ($('.credit-card-rewards-result-wrapper').length > 0) {
      $('.credit-card-rewards-result-wrapper .cta-show-more').on('click', function (e) {
        e.preventDefault();
        dataSubmit();
        ajaxPull();

        // scroll to content
        if (window.matchMedia("(min-width: 992px)").matches) {
          $(window).scrollTop(currScrollPos);
        }
      });
    }

    $(window).scroll(function () {
      currScrollPos = $(window).scrollTop();
    });

    function render(data) {
      $.ajax({
        type: 'GET',
        url: '/bin/hlb/creditcardrewards',
        cache: false,
        dataType: "json",
        data: data,
        success: function success(result) {

          data = result;
          var hiddenItem = $('.credit-card-rewards-result-item-group .credit-card-rewards-result-item.hidden');

          // check if no result
          if (data.result.length != 0) {
            $('.credit-card-rewards-no-results-wrapper').addClass('hidden');
            $('.credit-card-rewards-result-wrapper').removeClass('hidden');
          } else {
            $('.credit-card-rewards-no-results-wrapper').removeClass('hidden');
            $('.credit-card-rewards-result-wrapper').addClass('hidden');
          }

          if (data.completed == false && hiddenItem.length == 0) {
            // result not completely loaded yet
            completed = false;
            rewardsTemplate(data);
            if (data.result.length > 8) {
              call = false;
            }

            $('.credit-card-rewards-result-wrapper .cta-show-more').removeClass('hidden');
          } else if (data.completed == true) {
            completed = true;

            call = false;

            // result completely loaded
            if (data.result.length <= 8) {
              rewardsTemplate(data);
              var hiddenItem = $('.credit-card-rewards-result-item-group .credit-card-rewards-result-item.hidden');
              $('.credit-card-rewards-result-wrapper .cta-show-more').addClass('hidden');
            } else {
              rewardsTemplate(data);
              var hiddenItem = $('.credit-card-rewards-result-item-group .credit-card-rewards-result-item.hidden');
              $('.credit-card-rewards-result-wrapper .cta-show-more').removeClass('hidden');
            }
          }

          $('.credit-card-rewards-result-item-group .credit-card-rewards-result-item.hidden').slice(0, 8).removeClass('hidden');

          //find out more event
          $('.credit-card-rewards-result-item .cta-find-more').on('click', function (e) {
            e.preventDefault();
            var modalTitle = $(this).data('header');
            var modalContent = $(this).data('desc');
            $('.modal-blue-title').html(modalTitle);
            $('.modal-blue-content').html(decodeUrlParameter(modalContent));
          });

          //currShowMorePos = $('.credit-card-rewards-result-wrapper .cta-show-more')? $('.credit-card-rewards-result-wrapper .cta-show-more').offset().top : 0;
        },
        error: function error(obj) {
          alert('fail');
        }
      });
    }

    function rewardsTemplate(data) {
      data = data.result.length ? data.result : undefined;
      var source = '{{#each this}}';
      source += '<div class="credit-card-rewards-result-item hidden">';
      source += '<img class="img-responsive" src="{{image}}" alt="{{imageAlt}}">';
      source += '<div class="card-info-listing-with-bold-line dax-light type3">';
      source += '<span class="bar padding-top-s block"></span>';
      source += '<div class="padding-m">';
      source += '<div class="margin-bottom-l">';
      source += '<p class="noto fs-4 uppercase color-txt-dark-grey bold margin-bottom-xs">{{category}}</p>';
      source += '<h6 class="dax-medium margin-bottom-xs">{{name}}</h6>';
      source += '<p class="noto color-txt-dark-grey fs-2">{{desc}}</p>';
      source += '</div>';
      source += '<div class="row">';
      source += '<div class="col-xs-5">';
      source += '<div class="margin-vertical-xxs">';
      source += '<span class="ico-online-redemption icon-s margin-right-m margin-sm-right-xl color-txt-blue"></span>';
      source += '<span class="ico-self-service-phone-banking icon-s color-txt-blue"></span>';
      source += '</div>';
      source += '</div>';
      source += '<div class="col-xs-7">';
      source += '<p class="noto color-txt-dark-grey fs-2">{{pointsLabel}}: {{principalPoints}}<br>{{codeLabel}}: {{principalCode}}</p>';
      source += '</div>';
      source += '<div class="col-xs-12">';
      source += '<hr class="margin-vertical-s">';
      source += '</div>';
      source += '</div>';
      source += '<div class="row">';
      source += '<div class="col-xs-5">';
      source += '<div class="margin-vertical-xxs">';
      source += '<span class="ico-fax-redemption icon-s margin-right-m margin-sm-right-xl color-txt-blue"></span>';
      source += '<span class="ico-contact-center icon-s color-txt-blue"></span>';
      source += '</div>';
      source += '</div>';
      source += '<div class="col-xs-7">';
      source += '<p class="noto color-txt-dark-grey fs-2">{{pointsLabel}}: {{supplementaryPoints}}<br>{{codeLabel}}: {{supplementaryCode}}</p>';
      source += '</div>';
      source += '<div class="col-xs-12">';
      source += '<hr class="margin-vertical-s">';
      source += '</div>';
      source += '</div>';
      source += '<div class="row">';
      source += '<div class="col-xs-5">';
      source += '<div class="margin-vertical-xxs">';
      source += '<span class="ico-fast-track icon-m margin-right-m margin-sm-right-xl color-txt-blue"></span>';
      source += '</div>';
      source += '</div>';
      source += '<div class="col-xs-7">';
      source += '<p class="noto color-txt-dark-grey fs-2">{{pointsLabel}}: {{fastTrackPoints}}<br> {{codeLabel}}: {{fastTrackCode}}</p>';
      source += '</div>';
      source += '</div>';
      source += '{{#if uspCopy}}';
      source += '<div class="row">';
      source += '<div class="col-xs-12">';
      source += '<a href="#" data-toggle="modal" data-target="#blueModal" data-header="{{uspHeader}}" data-desc="{{uspCopy}}" class="cta-find-more color-txt-blue btn-txt-with-arrow fs-4 dax-medium bold uppercase float-right margin-l" title={{ctaLabel}}>{{ctaLabel}}</a>';
      source += '</div>';
      source += '</div>';
      source += '{{/if}}';
      source += '</div>';
      source += '</div>';
      source += '</div>';
      source += '{{/each}}';

      var template = Handlebars.compile(source);
      $('.credit-card-rewards-result-item-group').append(template(data));
    }

    function ajaxPull() {
      if (call == true) {
        render(data);
        
      } else {

        $('.credit-card-rewards-result-item-group .credit-card-rewards-result-item.hidden').slice(0, 8).removeClass('hidden');

        if ($('.credit-card-rewards-result-item-group .credit-card-rewards-result-item.hidden').length < 1) {
          if (!completed) {
            call = true;
            counter++;
          } else {
            $('.credit-card-rewards-result-wrapper .cta-show-more').addClass('hidden');
          }
        } else {
          if (!completed) call = false;
          $('.credit-card-rewards-result-wrapper .cta-show-more').removeClass('hidden');
        }
      }
    }

    function dataSubmit() {
      data = {
        locale: locale,
        counter: maxlength * counter,
        categories: JSON.stringify(selectedCategories),
        pointsRange: JSON.stringify(selectedPointRange),
        resourcePath: rewardListDataSource
