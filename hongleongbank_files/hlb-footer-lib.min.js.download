(function () {

  "use strict";

  window.carloaninst = function () {
    var myindex;
    var month;
    var tDownpayment;
    var tIntrate;
    var tCarprice;

    myindex = document.carloan.cliperiods.selectedIndex;
    month = document.carloan.cliperiods.options[myindex].value;
    tDownpayment = document.carloan.clidownpayment.value;
    tIntrate = document.carloan.cliintrate.value;
    tCarprice = document.carloan.clicarprice.value;

    $(".cli-loan-amount").text('MYR' + commaSeparateNumber(tCarprice - tDownpayment));
    $(".cli-interest-rate").text(tIntrate + '%');
    $(".cli-repayment-periods").text(month);

    if (!tCarprice) {
      $(".clicarprice-errorMsg").css("display", "block");
      $("#clicarprice").focus();
    }

    else {
      $(".clicarprice-errorMsg").css("display", "none");
    }

    if (!tDownpayment) {
      $(".clidownpayment-errorMsg").css("display", "block");
      $("#clidownpayment").focus();
    }

    else {
      $(".clidownpayment-errorMsg").css("display", "none");
    }


    if (!tIntrate) {
      $(".cliintrate-errorMsg").css("display", "block");
      $("#cliintrate").focus();
    }

    else {
      $(".cliintrate-errorMsg").css("display", "none");
    }

    //result box blocking condition if either one of fields are not filled
    if (!tCarprice || !tDownpayment || !tIntrate) {
      $(".car-loan-result").css("display", "none");
      $(".carloanapplybtn").css("display", "none");
      return false;
    }

    var year = month / 12;
    var loanprice = tCarprice - tDownpayment;
    var cInstallment = (loanprice * (tIntrate / 100) * year + loanprice) / month;
    window.scrollBy(0, 200);
    $(".cli-monthly-installment").text('MYR' + commaSeparateNumber(calcRound(cInstallment)));
    $(".carloancalculator .hidden-result span").text(tCarprice + ' ' + '|' + ' ' + tDownpayment + ' ' + '|' + ' ' + tIntrate + '%' + ' ' + '|' + ' ' + month);
    check_dpayment();

    if (tDownpayment == "") {
      $(".clidownpayment-errorMsg").css("display", "block");
      $(".car-loan-result").css("display", "none");
      $(".clidownpayment-errorMsg").html("The downpayment is insufficient. The minimum downpayment shall be 10% of car purchase price.");
      $(".carloanapplybtn").css("display", "none");
      document.carloan.clidownpayment.focus();
      document.carloan.clidownpayment.select();
    } else {
      $(".clidownpayment-errorMsg").css("display", "none");
      $(".car-loan-result").css("display", "block");
      $(".carloanapplybtn").css("display", "block");
    }

    check_dpayment();
  };

  function commaSeparateNumber(val) {
    while (/(\d+)(\d{3})/.test(val.toString())) {
      val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
    }
    return val;
  }


  function check_dpayment() {
    if (document.carloan.clidownpayment.value != "") {
      var margin = (document.carloan.clicarprice.value - document.carloan.clidownpayment.value) / document.carloan.clicarprice.value;
      if (margin > 0.9) {
        //alert("The downpayment is insufficient. The minimum downpayment shall be 10% of car purchase price.");
        $(".clidownpayment-errorMsg").css("display", "block");
        $(".car-loan-result").css("display", "none");
        $(".clidownpayment-errorMsg").html("The downpayment is insufficient. The minimum downpayment shall be 10% of car purchase price.");
        $(".carloanapplybtn").css("display", "none");
        document.carloan.clidownpayment.focus();
        document.carloan.clidownpayment.select();
      }
      else {
        $(".clidownpayment-errorMsg").css("display", "none");
        $(".car-loan-result").css("display", "block");
        $(".carloanapplybtn").css("display", "block");
      }
    }
  }

  function calcRound(num) {
    var result = Math.floor(num) + ".";

    var n = result.length;

    var cents = 100 * (num - Math.floor(num)) + 0.5;

    result += Math.floor(cents / 10);

    result += Math.floor(cents % 10);

    return result;
  }
})();

