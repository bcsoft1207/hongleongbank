      breadcrumbsHeight = $(".breadcrumb").innerHeight(),
      grandTitleHeight = $(".grand-title").innerHeight(),
      stickyTriggerPoint = $(".sales-kit-tab-wrapper .anchor-point-navigation-background"),
      offset = $(".sales-kit-tab-wrapper .anchor-point-navigation-background").offset().top,
      bursaSticky = false,
      navItems = (function () {
        var arr = [];
        $(".sales-kit-tab-wrapper .anchor-point-navigation .anchor-point-link").each(function () {
          arr.push($(this).attr("href"))
        });

        return arr;
      });

    navItems = navItems();

    //To not include breadcrumbs and grand title on sticky effect
    breadcrumbsHeight = 0,
      grandTitleHeight = 0;
    //To not include breadcrumbs and grand title on sticky effect

    $(window).on("scroll", function (e) {

      //Get current position
      var fromTop = this.pageYOffset,
        winOffset = $(window).scrollTop(),
        bursaNav = $("#bursa-new-nav-container").innerHeight(),
        salesKitTab = $(".sales-kit-tab-wrapper .anchor-point-navigation-background").innerHeight();
      var buffer = 0;

      var calcTop = header - 1 + breadcrumbsHeight + grandTitleHeight;

      if ($("#bursa-new-nav-container.sticky").length) {
        stickyTriggerPoint = $("#bursa-new-nav-container");
        bursaSticky = true;
        buffer = 6;
      }
      else {
        var newOffset = offset - salesKitTab;
      }

      if (bursaSticky) {
        calcTop += bursaNav;
      }


      e.stopPropagation();
      //Navbar Logic
      if (winOffset >= newOffset || $("#bursa-new-nav-container.sticky.fixed").length) {
        $(".sales-kit-tab-wrapper .anchor-point-navigation-background").addClass("custom-sticky");
        $(".sales-kit-tab-wrapper .anchor-point-navigation-background").css("top", (calcTop - buffer) + "px");
      }
      else {
        $(".sales-kit-tab-wrapper .anchor-point-navigation-background").removeClass("custom-sticky");
        $(".sales-kit-tab-wrapper .anchor-point-navigation-background").css("top", "0px");
      }
      //Navbar Logic

      calcTop += salesKitTab;

      //Sales Active Logic
      for (var i = 0; i < navItems.length; i++) {
        var elemPosition = $(navItems[i]).offset().top;
        //console.log(i, navItems[i], elemPosition);
        if ((calcTop + fromTop) >= elemPosition) {
          $(".sales-kit-tab-wrapper .anchor-point-navigation .anchor-point-link").removeClass("active");
          $(".sales-kit-tab-wrapper .anchor-point-navigation .anchor-point-link[href='" + navItems[i] + "']").addClass("active");
        }
      }
      //Sales Active Logic
    });

    $(".sales-kit-tab-wrapper .anchor-point-navigation .anchor-point-link").click(function (e) {

      e.stopPropagation();
      e.preventDefault();

      var bursaNav = $("#bursa-new-nav-container").innerHeight(),
        salesKitTab = $(".sales-kit-tab-wrapper .anchor-point-navigation-background").innerHeight();
      var buffer = 0;
      var calcTop = header + breadcrumbsHeight + grandTitleHeight;

      if ($("#bursa-new-nav-container.sticky").length) {
        stickyTriggerPoint = $("#bursa-new-nav-container");
        bursaSticky = true;

      } else {

        buffer = 6;
      }

      if (bursaSticky) {
        calcTop += bursaNav - 1;
        if (!$("#bursa-new-nav-container.sticky.fixed").length) {
          calcTop += bursaNav;
        }
      }
      calcTop += salesKitTab;
      if (!$(".sales-kit-tab-wrapper .anchor-point-navigation-background.custom-sticky").length) {
        calcTop += salesKitTab;
      }



      var target = $(this).attr("href");


      $('html, body').animate({
        scrollTop: Math.ceil($(target).offset().top - (calcTop - buffer))
      }, 'slow');



    });

  }
