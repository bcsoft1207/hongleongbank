function checkQuizAnswer(e, hexPath, nodePath) {
  var itemSha = $("input:radio[name='" + hexPath + "']:checked").data('input-s');
  $(e).siblings('.quiz-answer-error').addClass('hidden');

  if (itemSha) {
    $(e).addClass('disabled');
    $("input:radio[name='" + hexPath + "']").parent('.radio-group').addClass('disabled');
    $(e).siblings('.quiz-answer-msg').removeClass('hidden');

    $.ajax({
      type: 'GET',
      url: nodePath + '.checkanswer.' + itemSha + '.json',
      cache: false,
      success: function success(obj) {
        if (obj.errorCode == "000") {
          $("div[data-input-h='" + hexPath + "']").attr('data-input-t', obj.quiz.token);

          if (obj.quiz.isCorrect == "1") // correct answer - show correct answer explanation
          {
            $("p[data-exp-display='" + hexPath + "']").text(obj.quiz.explanation)
          } else if (obj.quiz.isCorrect == "0") // wrong answer - show wrong answer explanation
          {
            $("p[data-exp-display='" + hexPath + "']").text(obj.quiz.explanation)
          }

          surveyQuizAllowSubmit($("div[data-input-h='" + hexPath + "']").closest('form[data-form-h]').attr('data-form-h'));
        }



      },
      error: function error(obj) {
        //  alert('fail');
      }
    });
  } else {
    $(e).siblings('.quiz-answer-error').text($('#errMandatory').val());
    $(e).siblings('.quiz-answer-error').removeClass('hidden');
  }
}

