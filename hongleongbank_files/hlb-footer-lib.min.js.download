(function () {

  "use strict";

  var maxDSR_, commitment, propertyType, propertyRate_, financingAmount_, rate, months, installments, pv, outwardTotal, netIncome, monthlyInstallments_, newDSR_, affordable, effectiveRate, maxFinancing_, maxFinancing, maxMonthly, financingAmount, tenure, finalMonthly_, ir, np, pmt, total;

  Number.prototype.formatMoney = function (decPlaces, thouSeparator, decSeparator) {
    var n = this;
    var decPlaces = isNaN(decPlaces = Math.abs(decPlaces)) ? 2 : decPlaces;
    var decSeparator = decSeparator == undefined ? "." : decSeparator;
    var thouSeparator = thouSeparator == undefined ? "" : thouSeparator;
    var sign = n < 0 ? "-" : "";
    var i = parseInt(n = Math.abs(+n || 0).toFixed(decPlaces)) + "";
    var j = (j = i.length) > 3 ? j % 3 : 0;
    return sign + (j ? i.substr(0, j) + thouSeparator : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + thouSeparator) + (decPlaces ? decSeparator + Math.abs(n - i).toFixed(decPlaces).slice(2) : "");
  };

  function PMT(ir, np, pv, fv, type) {
    var pmt, pvif;
    fv || (fv = 0);
    type || (type = 0);
    if (ir === 0) return -(pv + fv) / np;
    pvif = Math.pow(1 + ir, np);
    pmt = -ir * pv * (pvif + fv) / (pvif - 1);
    if (type === 1) pmt /= 1 + ir;
    return -pmt;
  }

  function PV(rate, periods, payment, future, type) {

    var future = typeof future === 'undefined' ? 0 : future;
    var type = typeof type === 'undefined' ? 0 : type;

    rate = eval(rate);
    periods = eval(periods);

    if (rate === 0) {
      return -payment * periods - future;
    } else {
      return ((1 - Math.pow(1 + rate, periods)) / rate * payment * (1 + rate * type) - future) / Math.pow(1 + rate, periods);
    }
  }

  function check() {
    maxFinancing = $("#max-financing").val();
    if (maxFinancing < 0) {
      return "n.a.";
    }
    return maxFinancing;
  }

  function maxEligibleAmount() {
    maxDSR_ = parseInt($("#maximum-dsr").val());
    newDSR_ = parseInt($("#new-dsr").val());
    financingAmount = parseInt($("#financing-amount-2").val().replace(/,/g, ''));
    if (newDSR_ <= maxDSR_) {
      return financingAmount;
    };
    return check();
  }

  function finalMonthly() {
    if (check() == 'n.a.') {
      return 'n.a.';
    };
    var maxEligible = parseInt($("#max-eligible").val().replace(/,/g, ''));
    rate = $("#effective-rate").val() / 100;
    tenure = $("#tenure-2").val();
    finalMonthly_ = PMT(rate / 12, tenure * 12, maxEligible);
    return finalMonthly_;
  }

  function installmentCalc() {
    rate = $("#lending-rate").val() / 100;
    ir = rate / 12;
    np = $("#tenure").val() * 12;
    pv = parseInt($("#financing-amount").val());

    pmt = PMT(ir, np, pv);
    $("#monthly-installment-result").html(Math.ceil(pmt).formatMoney(0, ',', '.'));
  }

  function maxDSR() {
    maxDSR_ = 0.85;
    if ($("#gross-income").val() <= 3000) {
      maxDSR_ = 0.60;
    }
    return maxDSR_;
  }

  function monthlyCommitments() {
    total = 0;
    commitment = new Array();
    commitment.push(parseInt($("#outward-carloan").val()));
    commitment.push(parseInt($("#outward-houseloan").val()));
    commitment.push(parseInt($("#outward-personalloan").val()));
    commitment.push(parseInt($("#outward-creditcard").val()));
    commitment.push(parseInt($("#outward-other").val()));
    for (var i = commitment.length - 1; i >= 0; i--) {
      if (!isNaN(commitment[i]) && typeof commitment[i] == "number") {
        total += commitment[i];
      }
    };
    return total;
  }

  function calcPropertyRate() {
    propertyType = parseInt($("#property-type option:selected").val());
    propertyRate_ = 0.90;
    if (propertyType == 1) {
      propertyRate_ = 0.85;
    }

    return propertyRate_;
  }

  function calcFinancingAmount() {
    financingAmount_ = parseInt($("#property-price").val()) * calcPropertyRate();
    return financingAmount_;
  }

  function calcMonthlyInstallments() {
    rate = $("#effective-rate").val() / 100;
    months = $("#tenure-2").val() * 12;
    pv = parseInt($("#financing-amount-2").val().replace(/,/g, ''));
    installments = PMT(rate / 12, months, pv);
    return installments;
  }

  function newDSR() {
    outwardTotal = parseInt($("#outward-total").val().replace(/,/g, ''));
    netIncome = parseInt($("#net-income").val());
    monthlyInstallments_ = parseInt($("#monthly-installment").val().replace(/,/g, ''));

    newDSR_ = (monthlyInstallments_ + outwardTotal) / netIncome * 100;
    return newDSR_;
  }

  function affordableCommitment() {
    maxDSR_ = parseInt($("#maximum-dsr").val()) / 100;
    affordable = Math.ceil((maxDSR_ - outwardTotal / netIncome) * netIncome);
    return affordable;
  }

  function maximumFinancing() {
    effectiveRate = $("#effective-rate").val() / 100;
    maxFinancing_ = Math.floor(PV(effectiveRate / 12, months, -Math.abs(affordable)));
    return maxFinancing_;
  }

  function maximumMonthlyPayments() {
    effectiveRate = $("#effective-rate").val() / 100;
    months = $("#tenure-2").val() * 12;
    maxMonthly = Math.ceil(PMT(effectiveRate / 12, months, maximumFinancing()));
    return maxMonthly;
  }

  function updateValues() {
    //DO ALL CALCULATIONS
    $("#maximum-dsr").val(maxDSR() * 100);
    $("#outward-total").val(monthlyCommitments().formatMoney(0, ',', '.'));
    $("#property-rate").val(calcPropertyRate() * 100);
    $("#financing-amount-2").val(calcFinancingAmount().formatMoney(0, ',', '.'));
    $("#monthly-installment").val(Math.ceil(calcMonthlyInstallments()).formatMoney(0, ',', '.'));
    $("#new-dsr").val(Math.ceil(newDSR()));
    $("#affordable-commitment").val(affordableCommitment());
    $("#max-financing").val(maximumFinancing());
    $("#check").val(check());
    $("#max-monthly").val(maximumMonthlyPayments());
    $("#max-eligible").val(maxEligibleAmount());
    $("#final-monthly-installment").val(Math.ceil(finalMonthly()));

    // OUTPUT VALUES TO RESULTS BOX
    $("#max-eligible-result").html(parseInt($("#max-eligible").val()).formatMoney(0, ',', '.'));
    $("#final-monthly-result").html(parseInt($("#final-monthly-installment").val()).formatMoney(0, ',', '.'));
  }

  $(document).ready(function () {

    $(".installment-calculator input").on('change', function () {
      installmentCalc();
    });

    $(".prop-loan-calc input, .prop-loan-calc select").on('change', function () {
      updateValues();
    });
  });
})();
