      console.log('psDebitCardUsage=='+psDebitCardUsage);
    $(".payandsavecalculator .hidden-result span").text(psAverageDailyAccBalance + ' ' + '|' + ' ' + ($("#psDebitCardUsage").children("option:selected").text()) + ' ' + '|' + ' ' + ($("#ePaymentUsage").children("option:selected").text()) + ' ' + '|' + ' ' + ($("#shareTradingUsage").children("option:selected").text()));
    // document.getElementsByClassName("average-daily-error-msg")[0].classList.add("d-none");
    //$('.accbalance-errorMsg').hide();
    $(".accbalance-errorMsg").css("display", "none");


    if (psAverageDailyAccBalance == "") {
      //Throw Error
      document.payandsave.psAverageDailyAccBalance.focus();
      $('.accbalance-errorMsg').css("display", "block");
      //document.carloan.clidownpayment.select();
      document.getElementById("paysaveresult").classList.add("d-none");
      //document.getElementsByClassName("average-daily-error-msg")[0].classList.remove("d-none");
    }
    else {
      //Calc Savings Interest
      var savingsValue = psAverageDailyAccBalance;
        console.log('savingsValue=='+savingsValue);
      // var savingsRate = checkSavingsRate(savingsValue);
      var savingsRate = transactionUsage;
      //min and max eligible
      if (savingsValue > 100000) {
           savingsValue = 100000;
      } else if (savingsValue < 1000) {
           savingsRate = transactiondefaultrate;
      }      
     
      var savingsInterestResult = ((savingsValue * savingsRate) / 100) / 12;
      savingsValue = psAverageDailyAccBalance;

      if (maxSavingsInterest != 0 && savingsInterestResult > maxSavingsInterest) {
        savingsInterestResult = maxSavingsInterest;
      }

      $("#savingsrate").html(calcRound(savingsRate));
      $("#savingsinterestresult").html(addCommas(calcRound(savingsInterestResult)));

      var bonusInterestResult = 0;
      var bonusRate = 0;
      if (psDebitCardUsage) {
        bonusRate = Number.parseFloat(psDebitCardUsage);

        bonusInterestResult = ((savingsValue * bonusRate) / 100) / 12;

        if (maxBonusInterest != 0 && bonusInterestResult > maxBonusInterest) {
          bonusInterestResult = maxBonusInterest;
        }

        $("#bonusrate").html(calcRound(bonusRate));
        $("#bonusinterestresult").html(addCommas(calcRound(bonusInterestResult)));
      }
      else {
        $("#bonusrate").html("0.00");
        $("#bonusinterestresult").html("0.00");
      }

      var extraInterestResult = 0;
      var extraRate = 0;
      if (ePaymentUsage) {
        extraRate = Number.parseFloat(ePaymentUsage);

        extraInterestResult = ((savingsValue * extraRate) / 100) / 12;

        if (maxExtraInterest != 0 && extraInterestResult > maxExtraInterest) {
          extraInterestResult = maxExtraInterest;
        }


        $("#extrarate").html(calcRound(extraRate));
        $("#extrainterestresult").html(addCommas(calcRound(extraInterestResult)));
      }
      else {
        $("#extrarate").html("0.00");
        $("#extrainterestresult").html("0.00");
      }

      var shareInterestResult = 0;
      var shareRate = 0;
      if (shareTradingUsage) {
        shareRate = Number.parseFloat(shareTradingUsage);

        shareInterestResult = ((savingsValue * shareRate) / 100) / 12;
        $("#sharerate").html(calcRound(shareRate));
        $("#shareinterestresult").html(addCommas(calcRound(shareInterestResult)));
      }
      else {
        $("#sharerate").html("0.00");
        $("#shareinterestresult").html("0.00");
      }

      var totalInterestResult = (savingsInterestResult + bonusInterestResult + extraInterestResult + shareInterestResult);
      var totalRate = calcRound((savingsRate + bonusRate + extraRate + shareRate));

      $("#totalrate").html(totalRate);
      $("#totalinterestresult").html(addCommas(calcRound(totalInterestResult)));

      document.payandsaveresult.psResult.value = addCommas(calcRound(totalInterestResult * 12));

      document.getElementById("paysaveresult").classList.remove("d-none");

      $('html, body').animate({
        scrollTop: $('#paysaveresult').offset().top - $('.header-bar-overall').height() - 15
      }, 500);
    }
  }

  function calcRound(value) {
    return Number.parseFloat(value).toFixed(2);
  }
  function checkSavingsRate(value) {
    var rate;
    for (var i = 0; i < savingsInterest.length; i++) {
      var data = savingsInterest[i];

      if ((value >= data.from && !data.to) || (value >= data.from && value <= data.to)) {
        rate = data.rate;
        break
      }
    }
    return rate;
  }

  window.addDecimal = function () {
    document.payandsave.psAverageDailyAccBalance.value = addCommas(document.payandsave.psAverageDailyAccBalance.value.split(",").join(""));
  }
  function addCommas(value) {
    return value.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  }
  window.checkDot = function (evt) {
    var charCode = evt.which ? evt.which : event.keyCode;

    if (charCode == 46 || charCode == 13) return false;

    return true;
  }
