          scrollTop: 0
        }, 600);
        return false;
      });

      // Switching Search Icon and Clear text
      $(document).on('click', '.search-submit, .search-filter-1, .search-filter-2', function () {
        if ($('.search-input').val() !== '') {
          $('.search-input').blur();
          $('.search-keyword-suggestion').addClass('hide');
          performSearch($('.search-input').val(), selectedSectionChecker('search-filter-1'), selectedSectionChecker('search-filter-2'));
        }
      });

      // Trigger keyword suggestion
      var keywords;
      var lastLetter;
      var appendKeywordList = function () {
        var suggestion = [];
        $.each(keywords, function (i, v) {
          var pattern = new RegExp('(?:^|\\s)' + getRegexUnicode($('.search-input').val()), 'gi');
          if (pattern.test(v)) {
            var textPattern = new RegExp('(?:^|\\s)' + getRegexUnicode($('.search-input').val()) + '\\w*', 'gi');
            var matchedText = v.match(textPattern);
            var str = v;
            $.each(matchedText, function (index, value) {
              str = str.replace(value, '<span>' + value + '</span>');
              str = '<a class="list-group-item cursor-pointer">' + str + '</a>';
            });
            suggestion.push(str);
          }
        });

        if (suggestion.length > 0) {
          $('.search-keyword-suggestion').removeClass('hide');

          for (var i = 0; i < suggestion.length; i++) {
            if (i < $('#keywordListNumber').val()) {
              $('.search-keyword-suggestion .list-group').append(suggestion[i]);
            }
          }
          // Keyword selected
          $('.search-keyword-suggestion .list-group-item').on('click', function () {
            $('.search-keyword-suggestion').addClass('hide');
            $('.search-input').val($(this).text());
            $('.search-submit').trigger('click');
          });
        }
      }
      $('.search-input').on('keyup focus', function (e) {
        /* var keycode = e.keyCode;
         var valid = (keycode > 47 && keycode < 58) || // number keys
             keycode == 32 || keycode == 13 || keycode == 8 || // spacebar & return key(s) (if you want to allow carriage returns)
             (keycode > 64 && keycode < 91) || // letter keys
             (keycode > 95 && keycode < 112) || // numpad keys
             (keycode > 185 && keycode < 193) || // ;=,-./` (in order)
             (keycode > 218 && keycode < 223); // [\]' (in order)
             */

        $('.search-keyword-suggestion .list-group').empty();
        if ($('.search-input').val().length > 0) {
          if (!keywords || keywords.length == 0 || (!lastLetter || $('.search-input').val().substr(0, 1) != lastLetter)) {
            // Get list of keywords
            var url = '/bin/hlb/SearchKeywordServlet.' + $('#keywordSiteName').val() + '.' + $('#keywordCountryCode').val() + '.' + $('#keywordLanguageCode').val() + '.' + $('.search-input').val().substr(0, 1) + '.json';

            // send ajax request to server
            $.ajax({
              url: url,
              accepts: "application/json; charset=utf-8",
              contentType: "application/x-www-form-urlencoded; charset=UTF-8",
              dataType: "JSON",
              type: "GET",
              cache: false
            }).done(function (response) {
              if (response.errorCode == "000") {
                keywords = response.keyword;
                lastLetter = $('.search-input').val().substr(0, 1);
                appendKeywordList();
              }

            }).fail(function (request, status, err) {
              console.log(status);
            });
          } else {

            appendKeywordList();
          }
        } else {

          $('.search-keyword-suggestion').addClass('hide');
        }

      });
    }
  });

  function performSearch(keyword, section, searchCategory) {
