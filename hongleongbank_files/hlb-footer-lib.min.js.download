            $('.header-bar-overall').css('top', '0');
            $('.content-main').css('margin-top', '65px');
            $('.mega-nav').css('top', '66px');
            $('.search-container').css('top', '66px');
            isCloseReminder = true;
            return;
          }

          //reminder enddate must after currentdate.
          var enddate = configEle.attr("data-enddate");
          var enddate_ = moment(enddate).format("YYYY/MM/DD hh:mm:ss A");

          if (enddate !== "" && !moment(enddate_).isAfter(new Date())) {
            $('#app-reminder').css('display', 'none');
            $('.header-bar-overall').css('top', '0');
            $('.content-main').css('margin-top', '65px');
            $('.mega-nav').css('top', '66px');
            $('.search-container').css('top', '66px');
            isCloseReminder = true;
            return;
          }

          var lastDisplayTime = localStorage.getItem("lastDisplayTime");
          // first time display or last modified date changed.   
          if (null === lastDisplayTime || "null" === lastDisplayTime || (lastModified_) !== (lastModified)) {
            setDownloadLink(configEle);
            $('#app-reminder').css('display', 'block');
            lastDisplayTime = moment(new Date()).format("YYYY/MM/DD hh:mm:ss A");
            localStorage.setItem("lastDisplayTime", lastDisplayTime);
            localStorage.setItem("lastModified", lastModified);
            localStorage.removeItem("isClosed");
            return;

          }


          var expiredTime = configEle.attr("data-expriedtime");
          var countDiff = moment(new Date()).diff(lastDisplayTime, "hours");
          var isClosed = localStorage.getItem("isClosed");

          if ((null === isClosed || "null" == isClosed) || countDiff >= expiredTime) {
            setDownloadLink(configEle);
            $('#app-reminder').css('display', 'block');
            lastDisplayTime = moment(new Date()).format("YYYY/MM/DD hh:mm:ss A");
            localStorage.setItem("lastDisplayTime", lastDisplayTime);
            localStorage.setItem("lastModified", lastModified);
            localStorage.removeItem("isClosed");
            return;
          } else {
              $('#app-reminder').css('display', 'none');  
              $('.mega-nav').css('top', '66px');  
              $('.search-container').css('top', '66px');  
              if($(".app-slider").length === 0){  
                  //no app slider 
                  $('.header-bar-overall').css('top', '0'); 
                  $('.content-main').css('margin-top', '65px'); 
              }
              isCloseReminder = true;
          }

        } else {
          //reminder disabled 
          $('#app-reminder').css('display', 'none');  
          //$('.header-bar-overall').css('top', '0'); 
          //$('.content-main').css('margin-top', '65px'); 
          $('.mega-nav').css('top', '66px');  
          $('.search-container').css('top', '66px');  
          if($(".app-slider").length === 0){  
              $('.header-bar-overall').css('top', '0'); 
              $('.content-main').css('margin-top', '65px'); 
          }
          //in case slider jsp fail to load styles
          //else{
          //    $('.header-bar-overall').css('top', $(".app-slider").outerHeight()+1); 
          //    $('.content-main').css('margin-top', $(".app-slider").outerHeight()+66); 
          //}
          isCloseReminder = true;

        }

      });

      // fix product listing menu display previous selected.    
      if (window.location.hash) {
        var anchorLink = window.location.hash;
        var sectionSelector = "";
        sectionSelector = anchorLink.replace(/#section-|-1/g, '') + '"]';
        sectionSelector = sectionSelector.replace("#", "");
        setTimeout(function () {
          $('a[href="#' + sectionSelector).click();
          $("#dropdownMenu1 .product-category-link-span").text($('a[href="#' + sectionSelector).attr("title"));

        }, 300);

      }

    }


  })

  function setDownloadLink(element) {
    var installLink = "";
    if (isMobile.iOS()) {
      installLink = element.attr("data-iosLink");
    } else if (isMobile.Android()) {
      installLink = element.attr("data-androidLink");
    }
    $("#download_btn").attr("href", installLink);
  }

  window.onresize = function (event) {

    if (!isMobile.any()) {

      $('#app-reminder').css('display', 'none');
      $('.header-bar-overall').css('top', '0');
      $('.content-main').css('margin-top', '65px');
      $('.mega-nav').css('top', '66px');
      $('.search-container').css('top', '66px');
    }else{
      if(!isCloseReminder){
        $('#app-reminder').css('display', 'block');
      }      
    }

  };

  window.onscroll = function () {
      if($(".app-slider").length < 1){  
          myFunction()  
      }
  };

  function myFunction() {
    if (isMobile.any() && !isCloseReminder && $(".connect-reminder-container").length > 0) {
      if (document.body.scrollTop > 0 || document.documentElement.scrollTop > 0 ||
        isSearchNavOpen || isMegaNavOpen) {
        stickToTop = true;
      } else {
        stickToTop = false;
      }
      if (stickToTop) {
        $('.header-bar-overall').css('top', '0px'); 
        //$('.mega-nav').css('top', '66px');  
        $('.search-container').css('top', '66px');  
      } else {  
          if($("body").hasClass("mega-nav-open")){  
              $('.search-container').css('top', '66px');  
              $(".header-bar-overall").css("top","0");  
              $(".content-main").css("margin-top","66px");  
          }else{  
              $('.search-container').css('top', '66px');  
              $(".header-bar-overall").css("top","80px"); 
              $(".content-main").css("margin-top","146px"); 
          } 

      } 
    }
  }

})();


