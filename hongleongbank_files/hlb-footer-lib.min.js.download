//loan-affordability-calculaloanafford.js

(function () {

  "use strict";

  function laccalcRound(num) {

    var lacresult = Math.floor(num) + ".";

    var lacn = lacresult.length;

    var laccents = 100 * (num - Math.floor(num)) + 0.5;

    lacresult += Math.floor(laccents / 10);

    lacresult += Math.floor(laccents % 10);

    return lacresult;
  }

  window.laccal = function () {
    var lacmyindex;
    var lacMonth;
    var lactDownpayment;
    var lactIntrate;
    var lactafford;

    lacmyindex = document.loanafford.lacperiods.selectedIndex;
    lacMonth = document.loanafford.lacperiods.options[lacmyindex].value;
    lactDownpayment = document.loanafford.lacdownpayment.value;
    lactIntrate = document.loanafford.lacintrate.value;
    lactafford = document.loanafford.lacafford.value;

    $(".lac-interest-rate").text(lactIntrate + '%');
    $(".lac-rapayment-periods").text(lacMonth);

    if (!lactIntrate) {
      $(".intrst-rate-errorMsg").css("display", "block");
      $("#lacintrate").focus();
    } else {
      $(".intrst-rate-errorMsg").css("display", "none");
    }

    if (!lactDownpayment) {
      $(".downpayment-errorMsg").css("display", "none");
      $(".down-payment-errorMsg").css("display", "block");
      $("#lacdownpayment").focus();
    } else {
      $(".down-payment-errorMsg").css("display", "none");
    }

    if (!lactafford) {
      $(".afford-errorMsg").css("display", "block");
      $("#lacafford").focus();
    } else {
      $(".afford-errorMsg").css("display", "none");
    }


    //result box blocking condition if either one of fields are not filled
    if (!lactafford || !lactDownpayment || !lactIntrate) {
      $(".lac-result").css("display", "none");
      $(".lacapplyloan").css("display", "none");
      return false;
    }

    var lacyear = lacMonth / 12;

    var laccloanprice = lactafford * lacMonth / (1 + lactIntrate / 100 * lacyear);
    var lacccarprice = eval(laccloanprice) + eval(lactDownpayment);

    if (lactIntrate == "") return false;
    else {
      lactIntrate = document.loanafford.lacintrate.value; $(".lac-result").css("display", "block"); $(".lacapplyloan").css("display", "block");
    }

    $(".lac-loan-amount").text('MYR' + commaSeparateNumber(laccalcRound(laccloanprice)));
    $(".lac-car-price").text('MYR' + commaSeparateNumber(laccalcRound(lacccarprice)));
    window.scrollBy(0, 200);
    $(".affordabilitycalculator .hidden-result span").text(lactafford + ' ' + '|' + ' ' + lactDownpayment + ' ' + '|' + ' ' + lactIntrate + '%' + ' ' + '|' + ' ' + lacMonth);
    check_lacdpayment(laccloanprice, lacccarprice);
  };

  function commaSeparateNumber(val) {
    while (/(\d+)(\d{3})/.test(val.toString())) {
      val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
    }
    return val;
  }

  function check_lacdpayment(laccloanprice, lacccarprice) {
    var margin = laccloanprice / lacccarprice;
    if (margin > 0.9) {
      $(".down-payment-errorMsg").css("display", "none");
      $(".downpayment-errorMsg").css("display", "block").html("The downpayment is insufficient. The minimum downpayment shall be 10% of car purchase price.");
      //alert("The downpayment is insufficient. The minimum downpayment shall be 10% of car purchase price.");
      $(".lac-result").css("display", "none");
      $(".lacapplyloan").css("display", "none");

    } else {
      $(".downpayment-errorMsg").css("display", "none");
      $(".lac-result").css("display", "block");
    }
    //  document.loanafford.lacdownpayment.focus(); 
    //  document.loanafford.lacdownpayment.select();
  }
})();

