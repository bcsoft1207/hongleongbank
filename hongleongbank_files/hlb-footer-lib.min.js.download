              var distanceB = calculateDistance(pos.lat, pos.lng, Number(b.lat), Number(b.lng), "K");
              return distanceA - distanceB;
            });

            // Add distance to all object
            for (i = 0; i < data.length; i++) {
              data[i]["distance"] = calculateDistance(pos.lat, pos.lng, data[i]["lat"], data[i]["lng"], "K").toFixed(3);
            }

            data.forEach(function (item, i) {
