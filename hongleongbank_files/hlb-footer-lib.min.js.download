// News and Updates
(function () {

  "use strict";

  $(document).ready(function () {
    var maxPerSlide = 8,
      maxlength = 48,
      data = {},
      counter = 0,
      call = true,
      selectedCategories = [];

    if ($('.news-and-updates-filter').length > 0) {

      //Show all result when page loaded
      dataSubmit();
      ajaxPull();

      // Toggle dropdown
      $('.filter-result-btn').on('click', function (e) {
        e.preventDefault();
        $(this).siblings('.filter-expand-wrapper').toggleClass('hidden');
      });

      // Submit filter selection
      $('.news-update-filter-apply-cta').on('click', function (e) {
        e.preventDefault();
        counter = 0;
        selectedCategories = [];

        $(this).closest('form').find("input[type=checkbox]:checked").each(function () {
          selectedCategories.push($(this).val());
        });

        $(this).closest('.filter-expand-wrapper').toggleClass('hidden');
        $('.news-and-updates-item-group').empty();
        call = true;
        dataSubmit();
        ajaxPull();
        backToTop();
      });
      //update selector to be more specific just in case
      //filter remain same even after switch tab
        $(".newsandupdates-hlb .tab-selection .nav-item .nav-link").on("shown.bs.tab", function (e) {
            $('#news-updates-filter').trigger("reset");
            selectedCategories = [];
            var value = $(this).data("value");
            var value2 = $(".tab-selection .nav-item.active .nav-link").data("value");
            counter = 0;
            $('.news-and-updates-item-group').empty();
            call = true;
            dataSubmit();
            ajaxPull();
            backToTop();
        }); 
      // daterange
      $('.input-daterange').datepicker({
        format: 'yyyy-mm-dd',
        todayBtn: "linked"
      });

      $('.cta-calendar').on('click', function (e) {
        e.preventDefault();
        $(this).siblings('input').focus();
      });
    }

    if ($('.news-and-updates-wrapper').length > 0) {
      $('.news-and-updates-wrapper .cta-show-more').on('click', function (e) {
        e.preventDefault();
        ajaxPull();
      });
    }

    function render(data) {
      $.ajax({
        type: 'GET',
        url: '/bin/hlb/newsandupdates',
        cache: false,
        dataType: "json",
        data: data,
        success: function success(result) {
          data = result;
          var hiddenItem = $('.news-and-updates-item-group .news-and-updates-item.hidden');

          // check if no result
          if (data.result.length != 0) {
            $('.no-results-wrapper').addClass('hidden');
            $('.news-and-updates-wrapper').removeClass('hidden');
          } else {
            $('.no-results-wrapper').removeClass('hidden');
            $('.news-and-updates-wrapper').addClass('hidden');
          }

          if (data.completed == false && hiddenItem.length == 0) {
            // result not completely loaded yet
            newsTemplate(data);
          } else if (data.completed == true) {
            call = false;

            // result completely loaded
            if (data.result.length <= 8) {
              newsTemplate(data);
              var hiddenItem = $('.news-and-updates-item-group .news-and-updates-item.hidden');
              $('.news-and-updates-wrapper .cta-show-more').addClass('hidden');
            } else {
              newsTemplate(data);
              var hiddenItem = $('.news-and-updates-item-group .news-and-updates-item.hidden');
              $('.news-and-updates-wrapper .cta-show-more').removeClass('hidden');
            }
          }

          $('.news-and-updates-item-group .news-and-updates-item.hidden').slice(0, 8).removeClass('hidden');
        },
        error: function error(obj) {
          alert('fail');
        }
      });
    }

    function newsTemplate(data) {
      data = data.result.length ? data.result : undefined;
      var source = '{{#each this}}';
      source += '<a href="{{link}}" class="news-and-updates-item bg-white hidden" title="{{header}}">';
      source += '<div class="latest-lebel bg-blue absolute">';
      source += '<p class="dax-light ht6 color-txt-white">{{latestLabel}}</p>';
      source += '</div>';
      source += '<div class="news-and-updates-img-wrapper">';
      source += '<img class="news-and-updates-img-item" src="{{image}}" alt="{{header}}">';
      source += '</div>';
      source += '<p class="noto bold color-txt-dark-grey fs-4 margin-m uppercase">{{category}}</p>';
      source += '<h6 class="dax-medium color-txt-blue margin-side-m">{{header}}</h6>';
      source += '<p class="noto bold color-txt-dark-grey fs-4 margin-m">{{articleDate}}</p>';
      source += '<div class="color-txt-blue btn-txt-with-arrow fs-12 dax-medium bold uppercase float-right margin-vertical-l margin-right-3xl">{{readMoreLabel}}</div>';
      source += '</a>';
      source += '{{/each}}';
      var template = Handlebars.compile(source);
      $('.news-and-updates-item-group').append(template(data));
    }

    function ajaxPull() {
      if (call == true) {
        //$(".news-and-updates-wrapper").fadeOut();
       // $(".news-and-updates-wrapper").css("opacity",0.2);
        render(data);
        $('.news-and-updates-wrapper .cta-show-more').removeClass('hidden');
        setTimeout(function() {
          updateImage();
          //$(".news-and-updates-wrapper").fadeIn();
        }, 400);
      } else {
        $('.news-and-updates-item-group .news-and-updates-item.hidden').slice(0, 8).removeClass('hidden');
        if ($('.news-and-updates-item-group .news-and-updates-item.hidden').length < 1) {
          $('.news-and-updates-wrapper .cta-show-more').addClass('hidden');
        } else {
          $('.news-and-updates-wrapper .cta-show-more').removeClass('hidden');
        }
      }
    }

    function dataSubmit() {
      data = {
        locale: locale,
        counter: maxlength * counter,
        categories: JSON.stringify(selectedCategories),
        from: $("input[name=start]").val(),
        to: $("input[name=end]").val(),
        componentPath: componentPath,
        tab: $(".newsandupdates-hlb .tab-selection .nav-item.active .nav-link").data("value")
      };
      counter++;
    }
  });
})();

