//lead generate form hashtag procced

(function () {

  "use strict";

  function getSelector(str) {
    var selector = "";
    if (str.length > 0) {
      str = $.trim(str);
      selector = str.replace(/ /g, '-');
      selector = selector.toLowerCase();
    }
    return selector;
  }
  $(document).ready(function () {

    //attach product name as url selector
    if ($('a[data-url-selector]').length > 0) {
      var element = $("a[data-url-selector]:eq(0)");
      var selector = $(element).attr("data-url-selector");
      selector = getSelector(selector);
      var url = $(element).attr("href");
      if (url.length > 0) {
        var arrs = url.split("/");
        if (arrs.length > 0) {
          var currentPage = arrs[arrs.length - 1];
          if (currentPage.indexOf(".html") != -1) {
            var splitPage = currentPage.split(".html");
            var newPage = currentPage;
            if (splitPage.length >= 2) {
              newPage = splitPage[0] + "." + selector + ".html" + splitPage[1];
            }
            url = url.replace(currentPage, newPage);
            //$(element).attr("href", url);
            // for product banner
            $("#banner-apply-now-button").find("a").attr("href", url);
            $("a[data-url-selector]").each(function(i, obj) {
                $(this).attr("href", url);
            });
          }
        }
      }
    }

    // remove hash tag after close popup.
    if ($('.modal.modal-fullpage').length > 0) {
      $('.modal.modal-fullpage').on('hidden.bs.modal', function () {
        window.location.hash = "";
      })

      $('.modal.modal-fullpage').on('show.bs.modal', function () {
        console.log("Triger eligibility form");
        // put the DTM code here
      })
    }

    // trigger popup if url contains the hashtag.
    var hashTag = window.location.hash;
    hashTag = hashTag.replace("#", "");
    if (hashTag.length > 0) {
      if ($("div[data-hashtag='" + hashTag + "']").length > 0) {
        var modalID = $("div[data-hashtag='" + hashTag + "']").attr("id");
        $("#" + modalID).modal('show');
      }
    }

  });
})();

