        $('.search-keyword-suggestion').addClass('hide');
      });

      // Search input line transition when input focus
      $(".search-container .search-input").on('focus', function () {
        $('.search-transition-line').addClass("input-filled");
      }).on('blur', function () {
        if ($(this).val() == "") {
          $('.search-transition-line').removeClass("input-filled");
        }
      }).on('keypress', function (e) {
        var key = e.charCode || e.keyCode;
        if (key == 13) {
